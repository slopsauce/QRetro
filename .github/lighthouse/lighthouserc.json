{
  "ci": {
    "collect": {
      "numberOfRuns": 3
    },
    "assert": {
      "assertions": {
        "categories:performance": ["warn", {"minScore": 0.90}],
        "categories:accessibility": ["warn", {"minScore": 0.90}],
        "categories:best-practices": ["warn", {"minScore": 0.95}],
        "categories:seo": ["warn", {"minScore": 0.95}],
        "categories:pwa": "off"
      }
    },
    "upload": {
      "target": "temporary-public-storage"
    }
  }
}